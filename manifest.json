{
	"manifest_version": 2,
	"name": "CSDN-clean",
	"version": "1.0.0",
	"description": "",
	"page_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "我是pageAction",
		"default_popup": "popup.html"
	},
	"注释": "需要直接注入页面的JS:",
	"content_scripts": 
	[
		{
			"注释": "可以使用 <all_urls> 表示匹配所有地址",
			"matches": ["http://*.csdn.net/*", "https://*.csdn.net/*"],
			"js": ["js/main.js"],
			"run_at": "document_start"
		}
	]
}

